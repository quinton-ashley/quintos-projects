{
	"author": "quinton-ashley",
	"name": "quintos-games",
	"description": "Learn to code by making games in JavaScript using QuintOS!",
	"dependencies": {
		"java2js": "^1.0.5",
		"jquery": "^3.6.0",
		"p5": "^1.4.0",
		"quintos": "^1.6.85"
	},
	"license": "GPL-3.0-only",
	"main": "server.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/quinton-ashley/quintos-games.git"
	},
	"scripts": {
		"test": "node ./node_modules/quintos/server.js | open http://127.0.0.1:8081",
		"start": "node ./node_modules/quintos/server.js | start http://127.0.0.1:8081",
		"dev": "node ./node_modules/quintos/server.js 8081 --dev",
		"v": "npm version patch --force",
		"V": "npm version minor --force",
		"version": "git add -A",
		"postversion": "git push",
		"relink": "npm link ../../pkg/quintos | npm link ../../pkg/java2js"
	},
	"version": "1.0.24"
}
